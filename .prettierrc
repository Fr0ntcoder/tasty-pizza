{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/prisma/(.*)$",
		"^@/shared/ui",
		"^@/components/features(.*)$",
		"^@/components/layouts(.*)$",
		"^@/components/ui(.*)$",
		"^@/shared/hooks",
		"^@/shared/constants/(.*)$",
		"^@/shared/services/(.*)$",
		"^@/shared/store",
		"^@/shared/lib",
		"^@/@types/(.*)$",
		"^@/app/(.*)$",
		"^@/.*$",
		"^../(.*)$",
		"^./(?!.*\\.module\\.scss$).*$",
		"^./.*\\.module\\.scss$",
		"(.scss)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
